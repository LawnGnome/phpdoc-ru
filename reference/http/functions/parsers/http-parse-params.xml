 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Пример использования <function>http_parse_params</function></title>
    <programlisting role="php">
<![CDATA[
<?php
var_dump(http_parse_params("text/html; charset=\"utf8\""));
?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
object(stdClass)#1 (1) {
  ["params"]=>
  array(2) {
    [0]=>
    string(9) "text/html"
    [1]=>
    array(1) {
      ["charset"]=>
      string(4) "utf8"
    }
  }
}
]]>
    </screen>
   </example>
  </para>
 </refsect1>


