  </varlistentry>

  <varlistentry xml:id="ziparchive.constants.er-ok">
   <term>
    <constant>ZIPARCHIVE::ER_OK</constant>
     (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     Нет ошибок.
    </simpara>
   </listitem>
  </varlistentry>
